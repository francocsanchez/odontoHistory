<%- include('../partials/head') %> 

<h1>LISTADO DE OBRAS SOCIALES</h1>

<p><a href="/work-socials/create">+ NUEVA OBRA SOCIAL</a></p>
<div>
    <table class="default">
        <tr>
            <th>Nombre</th>
            <th>Creada</th>
            <th>Actualizada</th>
            <th></th>
        </tr>
        <% workSocials.forEach(workSocial=> { %>
            <tr>
                <td><%= workSocial.name%></td>
                <td><%= workSocial.createdAt%></td>
                <td><%= workSocial.updatedAt%></td>
                <td>
                    <a href="/work-socials/<%= workSocial._id %>/edit"> Editar</a> |
                    <a href="/work-socials/<%= workSocial._id %>/delete"> Eliminar</a>
                </td>
            </tr>
        <% });%>
    </table>
</div>

<%- include('../partials/footer') %>